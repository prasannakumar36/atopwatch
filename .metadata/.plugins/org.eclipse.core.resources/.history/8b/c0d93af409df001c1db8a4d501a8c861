package kumar;

public class second {
	public static void main(String[] args) {
		int n=345;
		String S=String.valueOf(n);
		int count=S.length();
		int sum=0;
		while(n!=0)
		{
			int j=n%10;
			if(count==2 || count==1)
			{
				sum=sum*10+j;
			}
			n/=10;
			count--;
		}
		System.out.println(sum);
		
	}

}
